{
  "name": "my-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "build": "eas build --profile development-simulator --platform ios --local",
    "start": "expo start --go",
    "docs": "STORYBOOK_ENABLED='true' npm run start",
    "test": "detox test --configuration ios.sim.debug --reuse --watchAll",
    "test:ci": "detox test --configuration ios.sim.debug --headless",
    "tsc:check": "tsc",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.+(json|js|ts|tsx)\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/native": "6.1.14",
    "@react-navigation/native-stack": "6.9.22",
    "@reduxjs/toolkit": "2.2.1",
    "expo": "50.0.8",
    "expo-constants": "15.4.5",
    "expo-linking": "6.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-redux": "9.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@storybook/addon-ondevice-actions": "7.6.15",
    "@storybook/addon-ondevice-controls": "7.6.15",
    "@storybook/react-native": "7.6.15",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.63",
    "babel-loader": "9.1.3",
    "babel-plugin-react-docgen-typescript": "1.5.1",
    "detox": "20.18.4",
    "detox-expo-helpers": "0.6.0",
    "eslint": "8.57.0",
    "eslint-config-universe": "12.0.0",
    "eslint-plugin-jest": "27.9.0",
    "expo-dev-client": "3.3.9",
    "expo-status-bar": "1.11.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-expo": "50.0.3",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}
