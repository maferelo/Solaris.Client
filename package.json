{
  "name": "my-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "build": "eas build --profile development-simulator --platform ios --local",
    "start": "expo start --go",
    "docs": "STORYBOOK_ENABLED='true' npm run start",
    "test": "jest --testPathIgnorePatterns=/e2e/",
    "test:e2e": "detox test --configuration ios.sim.debug --reuse --watchAll",
    "test:e2e:ci": "detox test --configuration ios.sim.debug --headless",
    "type:check": "tsc",
    "lint": "eslint .",
    "format": "prettier \"**/*.+(js|ts|tsx|json|yml)\"",
    "format:check": "npm run format -- --check",
    "format:fix": "npm run format -- --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/slider": "4.5.2",
    "@react-native-firebase/app": "20.1.0",
    "@react-navigation/native": "6.1.14",
    "@react-navigation/native-stack": "6.9.22",
    "@reduxjs/toolkit": "2.2.1",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.7",
    "@sentry/react-native": "~5.33.1",
    "@tanstack/react-query": "5.28.2",
    "axios": "1.7.2",
    "expo": "~51.0.31",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.26",
    "expo-linking": "~6.3.1",
    "expo-secure-store": "~13.0.2",
    "libphonenumber-js": "1.10.58",
    "promise": "8.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "7.51.0",
    "react-native": "0.74.5",
    "react-native-maps": "1.14.0",
    "react-native-root-toast": "3.6.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-redux": "9.1.0",
    "rxjs": "7.8.1",
    "zod": "3.22.4",
    "expo-build-properties": "~0.12.5"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@dev-plugins/react-navigation": "0.0.6",
    "@expo/ngrok": "4.1.3",
    "@storybook/addon-ondevice-actions": "7.6.15",
    "@storybook/addon-ondevice-controls": "7.6.15",
    "@storybook/react": "8.1.11",
    "@storybook/react-native": "7.6.15",
    "@types/jest": "29.5.12",
    "@types/react": "~18.2.79",
    "babel-loader": "9.1.3",
    "babel-plugin-react-docgen-typescript": "1.5.1",
    "detox": "20.18.4",
    "detox-cli": "20.0.0",
    "detox-expo-helpers": "0.6.0",
    "eslint": "8.57.0",
    "eslint-config-universe": "12.0.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-perfectionist": "2.7.0",
    "expo-dev-client": "~4.0.26",
    "expo-status-bar": "~1.12.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-expo": "~51.0.4",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "redux-devtools-expo-dev-plugin": "0.2.1",
    "start-server-and-test": "2.0.4",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  }
}
